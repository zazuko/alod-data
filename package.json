    "lindas-put": "env ENDPOINT=https://test.lindas-data.ch:8443/alod ./scripts/lindas-put-everything.sh",
    "zz-put": "env ENDPOINT=http://data.zazuko.com:5820/alod ./scripts/lindas-put-everything.sh"
